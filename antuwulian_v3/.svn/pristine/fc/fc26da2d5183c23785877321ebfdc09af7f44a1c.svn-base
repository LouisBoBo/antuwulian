<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/color_FFFFFF">

    <ImageView
        android:id="@+id/iv_fence"
        android:layout_width="@dimen/dp_38"
        android:layout_height="@dimen/dp_38"
        android:layout_marginStart="@dimen/dp_15"
        android:layout_marginTop="@dimen/dp_12"
        android:layout_marginBottom="@dimen/dp_12"
        android:src="@drawable/icon_fence" />

    <ImageView
        android:id="@+id/iv_right"
        android:layout_width="@dimen/dp_9"
        android:layout_height="@dimen/dp_16"
        android:layout_alignParentEnd="true"
        android:layout_centerVertical="true"
        android:layout_marginEnd="@dimen/dp_15"
        android:src="@drawable/icon_right_2" />

    <TextView
        android:id="@+id/tv_fence_name"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/dp_10"
        android:layout_marginTop="@dimen/dp_15"
        android:layout_marginEnd="@dimen/dp_10"
        android:layout_toStartOf="@+id/iv_right"
        android:layout_toEndOf="@+id/iv_fence"
        android:ellipsize="end"
        android:lines="1"
        android:text="@string/txt_name"
        android:textColor="@color/color_333333"
        android:textSize="@dimen/sp_15" />

    <TextView
        android:id="@+id/tv_fence_type"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/tv_fence_name"
        android:layout_marginStart="@dimen/dp_10"
        android:layout_marginEnd="@dimen/dp_10"
        android:layout_toStartOf="@+id/iv_right"
        android:layout_toEndOf="@+id/iv_fence"
        android:ellipsize="end"
        android:lines="1"
        android:text="@string/txt_type"
        android:textColor="@color/color_999999"
        android:textSize="@dimen/sp_12" />

    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/app_divider_p1"
        android:layout_below="@+id/iv_fence"
        android:background="@color/color_E5E5E5" />

</RelativeLayout>